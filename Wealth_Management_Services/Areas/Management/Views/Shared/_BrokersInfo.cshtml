@model IEnumerable<Wealth_Management_Services.Models.broker>

<table class="table">
    <tr>
        <td><b>Broker</b></td><td><b>Manager</b></td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.management.name)
            </td>

        </tr>
    }

</table>

